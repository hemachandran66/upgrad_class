import java.util.Scanner;

public class sum {
    public static void main(String[] args) {
        int num1,num2;
        Scanner input=new Scanner(System.in);
        System.out.println("Enter the two numers");
        num1= input.nextInt();
        num2= input.nextInt();
        System.out.println("Sum is " + (num1+num2));
    }
}
